{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"logs:CreateLogGroup",
				"logs:CreateLogStream",
				"logs:PutLogEvents"
			],
			"Resource": "arn:*:logs:*:*:*"
		},
        {
			"Effect": "Allow",
			"Action": "kendra:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "bedrock:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "sqs:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "dynamodb:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
			  "execute-api:ManageConnections"
			],
			"Resource": "arn:aws:execute-api:*:*:*/@connections/*"
		  }
	]
}